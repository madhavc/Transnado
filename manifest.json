{
    "manifest_version": 2,
    "name": "Control Shift Translate",
    "description": "Translate whatever you are writing into language of your choosing",
    "version": "0.1",
    "permissions": [
        "tabs"
    ],
    "content_scripts": [
        {
            "matches": ["http://*/", "https://*/"],
            "js": ["jquery.js", "keypress.min.js", "function.js"]
        }
    ]
}
